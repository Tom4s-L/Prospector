{
  "manifest_version": 3,
  "name": "LinkedIn prospecting automation",
  "description": "Extension pour prospecter sur LinkedIn",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["assets/content.js"],
    "run_at": "document_idle"
  }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ]
}
